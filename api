from fastapi import FastAPI

app = FastAPI()

@app.get("/")
async def welcome() -> dict:
    return {"message": "Hello, world"}

@app.get("/main")
async def welcome() -> dict:
    return {"message": "Main page"}



#Get - адрес в строке ?переменная=значение
#Post - формы - оформить заказ в магазине
#Put - заменить/обновить
#Delete - удалить

